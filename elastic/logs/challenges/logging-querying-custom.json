{% import "rally.helpers" as rally %}
{
  "name": "logging-querying-custom",
  "description": "Applies a query workload.",
  "parameters": {},
  "schedule": [
    {
      "name": "logging-queries-custom",
      "parallel": {
        "time-period": {{ p_query_time_period }},
        "warmup-time-period": {{ p_query_warmup_time_period }},
        "tasks": [
          { 
            "target-throughput": 80,
            "clients": 20,
            "operation": {
              "name": "Elasticsearch: POST /logs-*/_async_search - kafka - 1a.1",
              "operation-type": "search",
              "index": "logs-kafka*",
              "cache": "false",
              "request-params": {
                "batched_reduce_size": "64",
                "ignore_unavailable": "true",
                "track_total_hits": "true",
                "enable_fields_emulation": "true",
                "preference": "1650317360945"
              },
              "body": {
                "aggs": {
                  "2": {
                    "date_histogram": {
                      "field": "@timestamp",
                      "fixed_interval": "1h",
                      "time_zone": "UTC",
                      "min_doc_count": 1
                    },
                    "aggs": {
                      "3": {
                        "terms": {
                          "field": "kafka.log.trace.class",
                          "order": {
                            "_count": "desc"
                          },
                          "size": 10
                        }
                      }
                    }
                  }
                },
                "size": 50,
                "script_fields": {},
                "stored_fields": [
                  "*"
                ],
                "runtime_mappings": {},
                "query": {
                  "bool": {
                    "must": [],
                    "filter": [
                      {
                        "bool": {
                          "should": [
                            {
                              "exists": {
                                "field": "kafka.log.trace.class"
                              }
                            }
                          ],
                          "minimum_should_match": 1
                        }
                      },
                      {
                        "range": {
                          "@timestamp": {
                            "format": "strict_date_optional_time",
                            "gte": "2022-04-18T21:35:59.264Z",
                            "lte": "2022-04-18T21:50:59.264Z"
                          }
                        }
                      }
                    ],
                    "should": [],
                    "must_not": []
                  }
                }
              }
            }
        },
          {
            "target-throughput": 10,
            "clients": 5,
            "operation": {
            "name": "Elasticsearch: POST /logs-*/_async_search - nginx - 1.1",
            "operation-type": "search",
            "index": "logs-nginx.access*",
            "cache": "false",
            "request-params": {
              "batched_reduce_size": "64",
              "ignore_unavailable": "true",
              "track_total_hits": "true",
              "enable_fields_emulation": "true"
            },
            "body": {
              "size": 5000,
              "query": {
                "bool": {
                  "must": [
                    {
                      "range": {
                        "@timestamp": {
                          "gte": "2022-03-19T18:52:16.168Z",
                          "lte": "2022-05-28T19:07:16.168Z",
                          "format": "strict_date_optional_time"
                        }
                      }
                    },
                    {
                      "bool": {
                        "must": [
                          {
                            "query_string": {
                              "query": "data_stream.dataset:nginx.access",
                              "analyze_wildcard": true
                            }
                          }
                        ],
                        "filter": [],
                        "should": [],
                        "must_not": []
                      }
                    }
                  ],
                  "filter": [],
                  "should": [],
                  "must_not": []
                }
              },
              "aggs": {
                "61ca57f1-469d-11e7-af02-69e470af7417": {
                  "terms": {
                    "field": "url.original",
                    "order": {
                      "_count": "desc"
                    }
                  },
                  "aggs": {
                    "timeseries": {
                      "date_histogram": {
                        "field": "@timestamp",
                        "min_doc_count": 0,
                        "time_zone": "UTC",
                        "extended_bounds": {
                          "min": 1650480736168,
                          "max": 1650481636168
                        },
                        "fixed_interval": "2d"
                      },
                      "aggs": {
                        "61ca57f2-469d-11e7-af02-69e470af7417": {
                          "bucket_script": {
                            "buckets_path": {
                              "count": "_count"
                            },
                            "script": {
                              "source": "count * 1",
                              "lang": "expression"
                            },
                            "gap_policy": "skip"
                          }
                        }
                      }
                    }
                  },
                  "meta": {
                    "timeField": "@timestamp",
                    "panelId": "61ca57f0-469d-11e7-af02-69e470af7417",
                    "seriesId": "61ca57f1-469d-11e7-af02-69e470af7417",
                    "intervalString": "10s"
